<h1>hello edit pagess</h1>
{{#if categories}}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-6">
            <div class="card card-primary">     
                <form action="/admin/category/edit/{{categories.id}}" method="POST" enctype="multipart/form-data">      
                    <div class="card-body">    
                        <div class="form-group">
                            <label for="categoryName">Kategoriya nomi</label>
                            <input type="text" class="form-control" name="categoryName" id="categoryName" value="{{categories.categoryName}}">
                        </div>                   
                        <div class="form-group">
                            <label for="sortNumber">Kategoriya raqami</label>
                            <input type="number" class="form-control" name="sortNumber" id="sortNumber" value="{{categories.sortNumber}}">
                        </div>
                        <div class="form-group">
                            <label for="categoryIcon">Kategoriya ikonkasi</label>
                            <input type="file" class="form-control" name="categoryIcon" id="categoryIcon" value="{{categories.categoryIcon}}">
                        </div>
                    </div>
                    <div class="card-footer">
                        <button class="btn btn-primary" type="submit">O`zgartirish</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
