{{#if products.length}}
  {{#each products}}
    <div class="col-lg-3">
      <div class="card">
        <img src="/images/{{img}}" alt="{{img}}" />
        <div class="card-body" style="text-align: center;">
          <h1 class="card-title">Mahsulot nomi: <i>{{name}}</i> </h1>
          <br />
          <p class="card-title">Mahsulot narxi: <i>{{price}}</i> </p>
          <br />
          <hr />
          <div class="d-flex">
          </div>
        </div>
      </div>
    </div>
  {{/each}}
{{else}}
  <h1>Mahsulotlar yo'q</h1>
{{/if}}