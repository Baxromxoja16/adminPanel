{{#if products.length}}
<div class="row">
    {{#each products}}
    <div class="col-lg-3">
        <div class="card1">
            <img  src="/images/{{img}}" alt="{{img}}">
            <div class="card-body">
                <h1 class="card-title">Mahsulot nomi: <i>{{name}}</i></h1>
                <br>
                <p>Mahsulot narxi:  <i>{{price}}</i></p>
                <div class="d-flex">
                    <a href="/admin/product/delete/{{id}}" class="btn btn_del btn-danger">O'chirish</a>
                    <a href="/admin/product/edit/{{id}}" class="btn btn-warning">O'zgartirish</a>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
{{else}}
<h1>Mahsulotlar yo'q</h1>
{{/if}}